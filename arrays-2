var sizes = [40, 70, 90, 30, 60, 30, 55, 60];
var colors={
	r: 90,
	g: 50,
	b: 100,
}
	

function setup() {
  createCanvas(1000, 1000);

  noStroke();
    
  for (var i = 0; i < sizes.length; i += 1) {
		colorMode(RGB);
		// fill.forEach(randomColors(colors));
								 
    fill(colors.r,colors.g,colors.b);
    ellipse(50+i*100, height/2, sizes[i], sizes[i]);
  }
}

