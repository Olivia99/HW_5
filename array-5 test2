var sizes = [100,90,40,38.8,10,9.5,5.3,3.8];
var ellipses=[];
function setup(){
 createCanvas(1000,1000);
	for (var i=0; i<8;i++){
	ellipses[i]={
		x: 80+i*100,
		y: 500,
		d1: 90,
		d2: 90,
		drawEllipse: function(){
			fill(60,30,80);
			ellipse(this.x,this.y,this.d1,this.d2);
		},
		move: function(){
			this.y=this.y+100;
			}
		}
	}
}


function draw(){
	for(var i=0; i<8; i++){
		ellipses[i].drawEllipse();
		ellipses[i].move();
	}
}
